unit uFuncoes;

interface

uses
  System.SysUtils;

function LineBreak: string;
procedure dirCreate(const Dir: string);

implementation


function LineBreak: string;
begin
  Result := #13#10;
end;


procedure dirCreate(const Dir: string);
begin
  if not DirectoryExists(Dir) then
    ForceDirectories(Dir)
end;

end.
